<%= form_for @students do |f| %>

	<%= f.email_field :email, placeholder: "Email"  %><br/>
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password"  %><br/>
    <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Conform Password"  %>
    
 <%= f.submit  %>
<% end %>
